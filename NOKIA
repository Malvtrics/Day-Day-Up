SELECT
      BATCH_NUMBER ,
      TRANSFER_END_TIME ,
      PIP_FLAG ,
      PO_ENTRY_POINT ,
      IMPORTED ,
      IMPORT_REMARK ,
      PRICE_ID ,
      PRICE_TYPE ,
      VALID_FROM ,
      VALID_TO ,
      SHARE_PERCENT ,
      GLOBAL_PART_NUMBER ,
      LOCATION_CODE ,
      SYSTEM_SOURCE ,
      PRICE_SCOPE ,
      SUPPLIER_CODE ,
      SUPPLIER_NAME ,
      MANUFACTURER_CODE ,
      MANUFACTURER_NAME ,
      GLOBAL_SUPPLIER_NAME AS ENTERPRISE_ID
   FROM
      dbo.GPM_MODIAS_LOG_VW
   WHERE
      BATCH_NUMBER = '201707141707529424'
      AND GLOBAL_PART_NUMBER = '1AB002790012'; 
