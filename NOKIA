CREATE TABLE #TMP_GPRULE (RULE_CODE NVARCHAR(50), RULE_AREA NVARCHAR(50));
BULK INSERT #TMP_GPRULE FROM '\\135.236.65.21\Prodbak\gprule.txt' WITH ( FIELDTERMINATOR = '\t', ROWTERMINATOR = '\n') 
SELECT * FROM #TMP_GPRULE  WHERE  COMMODITY_CODE NOT IN 
(SELECT DISTINCT SUBCATEGORY_CODE FROM GPM_CATEGORY_MATRIX) --595
UPDATE R 
SET R.RULE_AREA = T.RULE_AREA
FROM GP_RULE R 
INNER JOIN #TMP_GPRULE T 
ON R.DOMAIN= 'NEW_STAGING_VAL' AND R.RULE_CODE = T.RULE_CODE;
DROP TABLE #TMP_GPRULE; 
